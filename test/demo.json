      "testGetArrayBuffer": { "type": "method", "value": [], "ret": "string", "err": "string" },
      "testGetBlob": { "type": "method", "value": [], "ret": "string", "err": "string" },
      "testGetJSON": { "type": "method", "value": [], "ret": "string", "err": "string" },
      "testPost": { "type": "method", "value": [], "ret": "string", "err": "string" },
      "testBlobPost": { "type": "method", "value": [], "ret": "string", "err": "string" },
      "testArrayBufferPost": { "type": "method", "value": [], "ret": "string", "err": "string" },
